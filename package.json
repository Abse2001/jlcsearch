{
  "name": "jlcpcb-parts-engine",
  "module": "index.ts",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "kysely": "^0.27.4",
    "kysely-codegen": "^0.17.0",
    "winterspec": "^0.0.96"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "start": "bun run scripts/start-server.ts",
    "generate:db-types": "bunx kysely-codegen --out-file ./lib/db/generated/kysely.ts --singular --dialect bun-sqlite --url ./db.sqlite3",
    "setup": "bun run setup:download-cache-fragments && bun run setup:extract-db && bun run setup:replace-db-file",
    "setup:download-cache-fragments": "bun scripts/download-cache-fragments.ts",
    "setup:extract-db": "7zz x .buildtmp/cache.zip",
    "setup:replace-db-file": "rm -f ./db.sqlite3 && mv ./cache.sqlite3 ./db.sqlite3"
  },
  "type": "module",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redaxios": "^0.5.1"
  }
}
